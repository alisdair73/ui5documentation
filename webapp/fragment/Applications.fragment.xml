<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
>
	<TableSelectDialog
		noDataText="No Applications Found"
		title="{detailHelper>/applicationsDialogTitle}"
		items="{/RelatedUI5Applications}"
	>
		<columns>
			<Column>
				<header>
					<Text text="Application Name" />
				</header>
			</Column>
			<Column>
				<header>
					<Text text="Component Id" />
				</header>
			</Column>
		</columns>
		<ColumnListItem>
			<cells>
				<Text text="{Description}" />
				<Text text="{ComponentID}" />
			</cells>
		</ColumnListItem>
	</TableSelectDialog>
</core:FragmentDefinition>